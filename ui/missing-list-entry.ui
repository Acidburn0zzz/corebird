<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.10"/>
  <template class="MissingListEntry" parent="GtkListBoxRow">
    <property name="visible">True</property>
    <property name="can_focus">True</property>
    <child>
      <object class="GtkStack" id="stack">
        <property name="transition_type">slide-up-down</property>
        <property name="visible">True</property>
        <child>
          <object class="GtkLabel" id="interrupted_label">
            <property name="visible">True</property>
            <property name="label" translatable="yes">The stream has been interrupted</property>
            <style>
              <class name="dim-label" />
            </style>
          </object>
          <packing>
            <property name="name">interrupted</property>
          </packing>
        </child>

        <child>
          <object class="GtkBox" id="box1">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="orientation">vertical</property>
            <property name="spacing">8</property>
            <property name="homogeneous">True</property>
            <child>
              <object class="GtkLabel" id="label1">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <property name="halign">center</property>
                <property name="valign">end</property>
                <property name="label" translatable="yes">Missing Content</property>
                <style>
                  <class name="dim-label"/>
                </style>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">True</property>
                <property name="position">0</property>
              </packing>
            </child>
            <child>
              <object class="GtkButton" id="load_missing_button">
                <property name="label" translatable="yes">Try to load</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
                <property name="halign">center</property>
                <property name="valign">start</property>
                <property name="relief">none</property>
                <signal name="clicked" handler="load_button_clicked_cb" swapped="false" />
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="fill">True</property>
                <property name="position">1</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="name">resumed</property>
          </packing>
        </child>

        <child>
          <object class="GtkBox" id="loading_box">
            <property name="visible">True</property>
            <property name="orientation">horizontal</property>
            <property name="spacing">10</property>
            <child>
              <object class="GtkSpinner" id="loading_spinner">
                <property name="visible">True</property>
                <property name="margin_left">20</property>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="loading_label">
                <property name="visible">True</property>
                <property name="wrap">True</property>
                <property name="label" translatable="yes">Loading...</property>
                <property name="valign">center</property>
                <style>
                  <class name="dim-label" />
                </style>
              </object>
            </child>
          </object>
          <packing>
            <property name="name">loading</property>
          </packing>
        </child>

        <child>
          <object class="GtkLabel" id="error_label">
            <property name="visible">True</property>
            <style>
              <class name="dim-label" />
            </style>
          </object>
          <packing>
            <property name="name">error</property>
          </packing>
        </child>
      </object>
    </child>
  </template>
</interface>
